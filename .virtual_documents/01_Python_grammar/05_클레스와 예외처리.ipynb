# class
class FourCal:
    def __init__ (self, first = 0, second = 0):
        self.first = first
        self.second = second
    # def setdata (self, first, second):
    #     self.first = first
    #     self.second = second
    def add (self):
        return (self.first + self.second)
    def sub (self):
        return (self.first - self.second)
    def mul (self):
        return (self.first * self.second)
    def div (self):
        return (self.first / self.second)


a = FourCal()
a.add()





class MoreFourCal(FourCal):
    def pow(self):
        return self.first ** self.second


a = MoreFourCal(1, 2)
a.add()


a = MoreFourCal(1, 2)
a.pow()





class SafeFourCal(FourCal):
     def div(self):
         if self.second == 0:  # 나누는 값이 0인 경우 0을 리턴하도록 수정
             return 0
         else:
             return self.first / self.second


a = SafeFourCal(3,0)
a.div()





