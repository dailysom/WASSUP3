marks = [90, 25, 67, 45, 80]  # 
for number in range(len(marks)): # len(marks) 여기에 5를 넣는다 그럼 변수에서 차례대로 0,1,2,3,4
    if marks[number] < 60: 
        continue # 넘어갈때 
    print("%d번 학생 축하합니다. 합격입니다." % (number+1))


