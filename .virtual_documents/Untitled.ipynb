# 이스케이프 코드(아래 4개 정도 사용만 )
a = "Life is too short, \nYou need Python" # 줄바꿈
print(a)


a = '''Life is too short,
You need Python''' # 줄바꿈
print(a)


a = "Life is too short, \tYou need Python" # 탭간격
print(a)


a = "Life is too short, \\You need Python" # \그대로 사용하고 싶을때 
print(a)


a = "Life \"is too short, You need Python" # 따옴표 자체를 보존할때 
print(a)


